{
  "name": "@loqalabs/loqa-audio-bridge",
  "version": "0.3.0",
  "description": "Production-grade Expo native module for real-time audio streaming with Voice Activity Detection and battery optimization",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "expo-module clean",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test": "jest",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android"
  },
  "keywords": [
    "expo",
    "react-native",
    "audio",
    "streaming",
    "vad",
    "microphone"
  ],
  "repository": "https://github.com/loqalabs/loqa",
  "bugs": {
    "url": "https://github.com/loqalabs/loqa/issues"
  },
  "author": "Loqa Labs <33356887+annabarnes1138@users.noreply.github.com> (https://github.com/loqalabs)",
  "license": "MIT",
  "homepage": "https://github.com/loqalabs/loqa#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "expo": "^54.0.18",
    "expo-module-scripts": "^5.0.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "prettier": "^3.0.0",
    "react": "^19.2.0",
    "react-native": "0.81.5",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "expo": ">=52.0.0",
    "expo-modules-core": "*",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "hooks/**/*.{ts,tsx}",
      "!**/*.d.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ],
    "testEnvironment": "node"
  }
}
